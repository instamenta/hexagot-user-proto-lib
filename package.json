{
  "name": "@instamenta/hexagot-user-proto-lib",
  "version": "1.0.1",
  "description": "user related protofile and corresponding generated code related to hexaledger project",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "npm run build",
    "generate": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./src/protos/generated/types --js_out=import_style=commonjs,binary:./src/protos/generated/ --grpc_out=grpc_js:./src/protos/generated -I ./src/protos/ ./src/protos/users.proto",
    "generate:depricated": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./generated/types --js_out=import_style=commonjs,binary:./generated/ --grpc_out=grpc_js:./generated -I ./ src/protos/users.proto",
    "remove": "rm ./src/protos/generated/*.js ./src/protos/generated/types/*.ts"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.8.15",
    "@grpc/proto-loader": "^0.7.7",
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "^20.3.1",
    "google-protobuf": "^3.21.2",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "ts-proto": "^1.149.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "proto",
    "grpc",
    "protobuf",
    "google-protobuf",
    "generated",
    "ts",
    "typescript",
    "js",
    "javascript"
  ],
  "author": "instamenta",
  "license": "MIT"
}
