{
  "name": "@instamenta/hexagot-user-proto-lib",
  "version": "1.0.0",
  "description": "user related protofile and corresponding generated code related to hexaledger project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./generated/types --js_out=import_style=commonjs,binary:./generated/ --grpc_out=grpc_js:./generated -I ./ ./users.proto"
  },
  "devDependencies": {
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "ts-proto": "^1.149.0",
    "@grpc/grpc-js": "^1.8.15",
    "@grpc/proto-loader": "^0.7.7",
    "google-protobuf": "^3.21.2",
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "^20.3.1"
  },
  "keywords": [
    "proto",
    "grpc",
    "protobuf",
    "google-protobuf",
    "generated",
    "ts",
    "typescript",
    "js",
    "javascript"
  ],
  "author": "instamenta",
  "license": "MIT"
}
